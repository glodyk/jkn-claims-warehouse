SELECT nmtindakan
FROM `bpjs-kediri.staging_nonkapitasi.non_kapitasi_stage2`
WHERE klasifikasi_kunjungan = 'Rawat Jalan Umum'
LIMIT 50;

SELECT *
FROM `bpjs-kediri.staging_nonkapitasi.non_kapitasi_stage2`
WHERE klasifikasi_kunjungan = 'Prolanis'
LIMIT 200;

SELECT *
FROM `bpjs-kediri.staging_nonkapitasi.non_kapitasi_stage2`
WHERE nmtindakan IS NULL
   OR TRIM(nmtindakan) = ''
LIMIT 200;

